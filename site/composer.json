{
    "name": "librecores/web",
    "license": "MIT",
    "type": "project",
    "description": "LibreCores Web Site",
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "App\\DataFixtures\\": "app/DataFixtures/",
            "App\\Tests\\": "tests/"
        },
        "files": [
            "app/AppKernel.php"
        ]
    },
    "require": {
        "php": "7.4.*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/symfony": "~3.4.0",
        "symfony/console": "~3.4.0",
        "symfony/process": "~3.4.0",
        "symfony/http-kernel": "~3.4.0",
        "symfony/cache": "~3.4.0",
        "symfony/swiftmailer-bundle": "~3.0",
        "symfony/monolog-bundle": "~3.1.0",
        "doctrine/orm": "^2.6",
        "doctrine/dbal": "^2.6",
        "doctrine/doctrine-bundle": "^1.8",
        "doctrine/doctrine-fixtures-bundle": "<2.5",
        "twig/extensions": ">=1.5.0",
        "sensio/distribution-bundle": "~5.0.18",
        "sensio/framework-extra-bundle": "~4.0.0",
        "knplabs/knp-markdown-bundle": "~1.3",
        "simplepie/simplepie": "^1.5",
        "php-amqplib/rabbitmq-bundle": "1.*",
        "hwi/oauth-bundle": "~0.5",
        "friendsofsymfony/user-bundle": "~2.0",
        "sensio/generator-bundle": "~3.1.0",
        "leafo/scssphp": "^0.6.6",
        "patchwork/jsqueeze": "^2.0",
        "exercise/htmlpurifier-bundle": "^3.0",
        "doctrine/doctrine-migrations-bundle": "^1.0",
        "knplabs/github-api": "^2.10",
        "php-http/guzzle6-adapter": "^1.1",
        "easycorp/easyadmin-bundle": "^1",
        "beberlei/doctrineextensions": "^1.0",
        "sentry/sentry-symfony": "^2.0",
        "php-http/httplug-bundle": "^1.10",
        "algolia/search-bundle": "^3.1",
        "spatie/yaml-front-matter": "^2.0",
        "ext-json": "*",
        "symfony/webpack-encore-bundle": "^1.6",
        "mgilet/notification-bundle": "^3.1",
        "stof/doctrine-extensions-bundle": "^1.3"
    },
    "scripts": {
        "auto-scripts": [
        ],
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-var-dir": "var",
        "symfony-bin-dir": "bin",
        "symfony-tests-dir": "tests"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "symfony/phpunit-bridge": "^4",
        "squizlabs/php_codesniffer": "^3.4",
        "escapestudios/symfony2-coding-standard": "3.x-dev",
        "mikey179/vfsstream": "^1.6",
        "sirbrillig/phpcs-variable-analysis": "^2.6",
        "sirbrillig/phpcs-import-detection": "^1.1"
    }
}
